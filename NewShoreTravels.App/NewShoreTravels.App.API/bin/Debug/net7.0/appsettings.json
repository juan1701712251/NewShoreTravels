{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Error"
    },
    "Loggers": {
      "NewShoreTravels.App.API.Controllers.JourneyController": {
        "Level": "Warning",
        "Path": "logs/error-{Date}.log",
        "LogEventIds": [ 2000, 2001 ],
        "Formatter": "NewShoreTravels.App.API.CustomLogFormatter, NewShoreTravels.App.API"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
